{
  "name": "digiventures/sdk",
  "description": "SDK oficial para integrar con la API de Digiventures",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Digiventures",
      "email": "dev@digiventures.la"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Digiventures\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Digiventures\\SDK\\Tests\\": "tests/"
    }
  }
} 